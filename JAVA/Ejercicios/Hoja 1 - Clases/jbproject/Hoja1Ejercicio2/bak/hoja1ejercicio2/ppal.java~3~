public class ppal {
  public ppal() {
  }
  public static void main(String[] args) {
libro libro1 = new libro("Las Aventuras de iTo","Alvaro",2000,"Anaya");
libro libro2 = new libro("Dani y el Dragón Verde","Eva Sannun",1969,"Espain");

libro1.GetTitulo();
libro1.GetAutor();
libro1.GetPublicado();
libro1.GetEditorial();

  }

}
