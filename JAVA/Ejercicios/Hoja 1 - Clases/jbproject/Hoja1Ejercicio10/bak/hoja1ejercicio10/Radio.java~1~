 * @version 1.0
 */

public class Radio {
//attributes
  private int frecuencia=80;
//metodes
// getters y setters
  public void setFrecuencia(boolean valor){
    if (valor==true) //si le pasamos un true, subimos 5 ptos la frecuencia
      frecuencia+=5;
    else // si por el contrario es false, bajamos 5 ptos la frecuencia
      frecuencia-=5;
  }


  public Radio() {}
}//main
