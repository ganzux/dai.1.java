public class Principal {
public static void main(String[] args) throws Exception {
  String opcion;
  BufferedReader in = new BufferedReader(new InputStreamReader(System.in));
Radio dial = new Radio();                // dial será un objeto de clase Radio
  do{
      System.out.println("8=Subir\n5=Salir\n2=Bajar\nDial está en " +
                         dial.getFrecuencia());
      opcion = in.readLine();
    }while(opcion.compareTo("5")!=0);//do
  }//main1
}//class
